{
	"parser": "babel-eslint",
	"extends": "eslint:recommended",
	"plugins": [
		"vue",
		"html"
	],
	"env": {
		"browser": true,
		"commonjs": true,
		"es6": true,
		"node": true
	},
	"parserOptions": {
		"ecmaFeatures": {
			"experimentalObjectRestSpread": true,
			"jsx": true
		},
		"sourceType": "module"
	},
	"rules": {
		"indent": [0],
		"quotes": [0],
		"semi": [
			"error",
			"always"
		],
		"prefer-const": [2],
		"spaced-comment": [2],
		"key-spacing": [2],
		"camelcase": [0],
		"comma-spacing": [2],
		"global-require": [1],
		"no-console": "off",
		"generator-star-spacing": "off",
		"vue/no-parsing-error": [2, { "x-invalid-end-tag": false }]
	}
}